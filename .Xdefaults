urxvt.font: xft:Monospace:pixelsize=11:antialias=true
urxvt.boldFont: xft:Monospace:pixelsize=11
URxvt*background:         #2e3440
URxvt*foreground:         #eeeeec
URxvt*selectToClipboard:  true
urxvt.cursorColor:  #202020
urxvt.color0:       #202020
urxvt.color8:       #606060
urxvt.color1:       #B91E2E
urxvt.color9:       #D14548
urxvt.color2:       #81957C
urxvt.color10:      #A7B79A
urxvt.color3:       #F9BB80
urxvt.color11:      #FAE3A0
urxvt.color4:       #356579
urxvt.color12:      #7491A1
urxvt.color5:       #2D2031
urxvt.color13:      #87314E
urxvt.color6:       #0B3452
urxvt.color14:      #0F829D
urxvt.color7:       #909090
urxvt.color15:      #FFF0F0
urxvt.fading: 0
urxvt.scrollBar: None
urxvt.termName: rxvt-256color
urxvt.internalBorder: 15
urxvt.scrollTtyKeypress: true
urxvt.scrollTtyOutput: true
urxvt.skipScroll:  false
urxvt.cursorBlink: true
urxvt.cursorColor: #4b5648
urxvt.buffered: false

urxvt.depth: 32
# rbga
# Simply just take regular RBG format (#123456) and split it up while padding
# with 0s and add transparency to alpha channel like so -> 1200/3400/5600/0000
#urxvt.background: rgba:2e00/3400/4000/c800
urxvt.background: rgba:0000/0000/0000/eeee
