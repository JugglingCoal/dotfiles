urxvt.font: xft:Monospace:size=6:antialias=true
urxvt.boldFont: xft:Monospace:bold:size=6:antialias=true
URxvt.letterSpace: -1
URxvt*background:         #000000
URxvt*foreground:         #eeeeec
URxvt*selectToClipboard:  true
urxvt.cursorColor:  #202020

URxvt*background: #000000
URxvt*foreground: #B2B2B2
! black
URxvt*color0:  #000000
URxvt*color8:  #686868
! red
URxvt*color1:  #B21818
URxvt*color9:  #FF5454
! green
#URxvt*color2:  #18B218 # sharper green
# duller green |
#              v
URxvt*color2:  #81957C
URxvt*color10: #54FF54
! yellow
URxvt*color3:  #B26818
URxvt*color11: #FFFF54
! blue
URxvt*color4:  #1818B2
URxvt*color12: #5454FF
! purple
URxvt*color5:  #B218B2
URxvt*color13: #FF54FF
! cyan
URxvt*color6:  #18B2B2
URxvt*color14: #54FFFF
! white
URxvt*color7:  #B2B2B2
URxvt*color15: #FFFFFF


urxvt.fading: 0
urxvt.scrollBar: None
urxvt.termName: rxvt-256color
urxvt.internalBorder: 10
urxvt.scrollTtyKeypress: true
urxvt.scrollTtyOutput: true
urxvt.skipScroll:  false
urxvt.cursorBlink: true
urxvt.cursorColor: #4b5648
urxvt.buffered: false

URxvt.underlineURLs:  True
URxvt.url-launcher:   /usr/bin/firefox
URxvt.clipboard.autocopy: true

urxvt.depth: 32
# rbga
# Simply just take regular RBG format (#123456) and split it up while padding
# with 0s and add transparency to alpha channel like so -> 1200/3400/5600/0000
#urxvt.background: rgba:2e00/3400/4000/c800
urxvt.background: rgba:0000/0000/0000/eeee
