background yes

#avoid flicker
double_buffer yes

#own window to run simultanious 2 or more conkys
own_window  yes
own_window_transparent yes #no
own_window_type desktop
own_window_hints undecorate,skip_taskbar,skip_pager,below ,sticky

#borders
draw_borders no
draw_graph_borders yes
border_width 1
border_margin 3

#shades
draw_shades no

#position
gap_x 0
gap_y 0
alignment top_left

#behaviour
update_interval 1

#colour
default_color 808080
#default_shade_color 000000
own_window_colour 364142 #000000
#font
use_xft yes
xftfont lime:size=7

# Force UTF8? note that UTF8 support required XFT
override_utf8_locale yes

#to prevent window from moving
use_spacer none
minimum_size 1366 0

#mpd
#mpd_host localhost
#mpd_port 6600

TEXT
${color green}OB ${color 8898ad}${time %d.%m.%Y %H:%M}     ${color}KERNEL: ${color 8898ad}$kernel ${color}    U/T: ${color 8898ad}$uptime ${color}    Temp: ${color 8898ad}${acpitemp}    ${color}CPU: ${color 8898ad}${cpu cpu0}% ${cpugraph 10,50 000000 ffffff}${color}    BAT: ${color 8898ad} ${battery_percent}% ${color}    UL: ${color 8898ad} ${upspeed eth0}${color 808080}/ ${color 8898ad}${totalup eth0} ${upspeedgraph eth0 10,50 000000 00ff00} ${color}    DL: ${color 8898ad} ${downspeed eth0}${color 808080}/ ${color 8898ad}${totaldown eth0} ${color}${downspeedgraph eth0 10,50 000000 ff0000}     $color RAM: ${color 8898ad}$memperc% (${color 8898ad}$mem)    ${color}/: ${color 8898ad}${fs_free_perc /}%    ${color} ${if_up eth0}${color}eth0: ${color 8898ad}${addr eth0}${endif}    ${if_up wlan0}${color}wlan0: ${color 8898ad}${addr wlan0}${endif}  ${if_up wlan0}${wireless_link_qual_perc eth1}%${endif}
